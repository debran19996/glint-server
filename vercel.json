{
  "crons": [
    {
      "path": "/api/cron/update-prices",
      "schedule": "*/20 * * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" },
        { "key": "Cache-Control", "value": "s-maxage=10, stale-while-revalidate=30" }
      ]
    }
  ]
}
